{
	"compilerOptions": {
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"module": "commonjs",
		"declaration": true,
		"esModuleInterop": true,
		"target": "es6",
		"moduleResolution": "node",
		"sourceMap": true,
		"outDir": "dist"
	},
	"lib": ["es2018"],
	"exclude": ["node_modules", "examples"],
}
